<%- include("header")-%>
<div class="container">
  <h1>SecretS<span>Save</span></h1>
  <p style="color: red;"><%=title%></p>
  <form action="/register" method="post">
    <input
      class="input"
      type="text"
      placeholder="Enter your name"
      name="username"
      required
    />
    <input
      class="input"
      type="text"
      placeholder="Enter your eamil"
      name="email"
      required
    />
    <input
      class="input"
      type="password"
      placeholder="Enter your password"
      name="password"
      required
    />
    <button class="btn" type="submit">Register</button>
  </form>
</div>
<%- include("footer")-%>
